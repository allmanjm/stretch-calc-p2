    <knowl>
        <content>
            <p><m mode="display">
            \lim_{x\to 0} \frac
                { {{Z1}} \left( 1-\cos(x) \right) }
                { {{denom1}} }
            </m></p>
        </content>
        <outtro>
            <p><m>{{ans1}}</m></p>
        </outtro>
    </knowl>
    <knowl>
        <content>
            <p><m mode="display">
            \lim_{x\to \infty} \frac
                { {{num2}} }
                { {{denom2}} }
            </m></p>
        </content>
        <outtro>
            <p><m>{{ans2}}</m></p>
        </outtro>
    </knowl>
</knowl>
